{
"manifest_version": 2,
    "name": "Obfuscator",
    "description": "Mushes any input field text",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}